{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "module": "esnext",
    "target": "es5",
    "noEmit": true,
    "strict": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "paths": {
      "#/*": ["./src/*"],
      "assets/*": ["./assets/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": []
}
